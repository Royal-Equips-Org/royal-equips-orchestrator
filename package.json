{
  "name": "royal-equips-orchestrator",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "lint": "pnpm -r lint",
    "typecheck": "tsc -p apps/command-center-ui/tsconfig.json --noEmit && tsc -p apps/orchestrator-api/tsconfig.json --noEmit && tsc -p apps/aira/tsconfig.json --noEmit && tsc -p apps/agent-executors/tsconfig.json --noEmit && tsc -p apps/api/tsconfig.json --noEmit && tsc -p packages/agents-core/tsconfig.json --noEmit && tsc -p packages/connectors/tsconfig.json --noEmit && tsc -p packages/obs/tsconfig.json --noEmit && tsc -p packages/shared/tsconfig.json --noEmit && tsc -p packages/shopify-client/tsconfig.json --noEmit && tsc -p packages/shared-types/tsconfig.json --noEmit",
    "test": "jest --runInBand --passWithNoTests",
    "prepare": "node -e \"process.exit((process.env.CI==='true'||process.env.NODE_ENV==='production')?0:1)\" || husky install",
    "fix-agent": "node scripts/fix-agent.mjs --watch",
    "start:orchestrator-api": "pnpm --filter @royal-equips/orchestrator-api start",
    "dev:orchestrator-api": "pnpm --filter @royal-equips/orchestrator-api dev",
    "start:aira": "pnpm --filter @royal-equips/aira start",
    "dev:aira": "pnpm --filter @royal-equips/aira dev",
    "start:api": "pnpm --filter @royal-equips/api start",
    "dev:api": "pnpm --filter @royal-equips/api dev"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@eslint/js": "9.35.0",
    "@types/jest": "^29.5.14",
    "chokidar": "^4.0.3",
    "diff": "^8.0.2",
    "eslint": "9.35.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "2.32.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "globals": "15.15.0",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.2",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.43.0",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
